# Configuration du Bot WhatsApp PestAlert

# Chemin de stockage des sessions WhatsApp
WHATSAPP_SESSION_PATH=./sessions

# Configuration API (pour futures intégrations)
API_BASE_URL=http://localhost:3000/api
API_KEY=your_api_key_here

# Configuration de l'analyse d'images (pour future intégration IA)
AI_ANALYSIS_ENDPOINT=https://api.pestalert.com/analyze
AI_API_KEY=your_ai_api_key_here

# Configuration des notifications d'urgence
EMERGENCY_PHONE=+33123456789
EMERGENCY_EMAIL=urgence@pestalert.com

# Configuration météo (pour future intégration)
WEATHER_API_KEY=your_weather_api_key_here

# Mode debug
DEBUG=true

# Numéros autorisés (optionnel, pour limiter l'accès)
# ALLOWED_NUMBERS=+33123456789,+33987654321
